<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
  
    <!-- A global light source -->

    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->

    <include>
      <uri>model://ground_plane</uri>
    </include>

    <actor name="actor">
      <skin>
        <filename>walk.dae</filename>
      </skin>
      <animation name="animation">
        <filename>moonwalk.dae</filename>
      </animation>
    </actor>

  </world>

</sdf>
